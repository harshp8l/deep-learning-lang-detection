Declare @startValue Int = 12  --""YOUR_VALUE"" [Ex: 8]
SELECT myColumn 
FROM @tempTable
ORDER BY (CASE WHEN myColumn < @startValue THEN myColumn+(select count(*) from @tempTable) ELSE myColumn END)
