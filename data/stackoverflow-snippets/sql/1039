Select EE.ID as ID_EXPEDIENTE, 
       EE.TIPO_DOCUMENTO,
       LISTAGG( EE.ANIO, "", "" ) WITHIN GROUP ( ORDER BY 1 ) AS ANIO
       LISTAGG( EE.NUMERO, "", "" ) WITHIN GROUP ( ORDER BY 1 ) AS NUMERO
       LISTAGG( CODIGO_REPARTICION_USUARIO, "", "" ) WITHIN GROUP 
           ( ORDER BY 1) AS CODIGO_REPARTICION_USUARIO,  
From GEDO_GED.DF_FORM_COMP_VALUE DFV  
left join EE_GED.DOCUMENTO D ON D.ID_TRANSACCION_FC = DFV.ID_TRANSACTION  
left join EE_GED.EE_EXPEDIENTE_DOCUMENTOS ED ON D.ID = ED.ID_DOCUMENTO  
left join EE_GED.EE_EXPEDIENTE_ELECTRONICO EE  ON EE.ID = ED.ID  
Where VALUE_STR = '"" + TextBox1.Text + ""'"" GROUP BY ee.id, ee.tipo_documento
