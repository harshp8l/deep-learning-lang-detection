import kantan.xpath._
import kantan.xpath.implicits._

val input = """"""
     | <application>
     |   <process type=""output"" size =""23"">
     |      <channel offset=""0""/>
     |      <channel offset=""4""/>
     |   </process>
     |   <process type=""input"" size =""16"">
     |      <channel offset=""20""/>
     |      <channel offset=""24""/>
     |   </process>
     | </application>
     | """"""

val inputs = input.evalXPath[List[Node]](xp""//process[@type='input']"")
