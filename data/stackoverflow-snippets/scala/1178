scala> :paste
// Entering paste mode (ctrl-D to finish)

def foo(str: String) = str + "", foo""
def bar(str: String) = str + "", bar""

val fmap = Map(""foo"" -> foo _, ""bar"" -> bar _)

fmap(""foo"")(""hello"")


// Exiting paste mode, now interpreting.

