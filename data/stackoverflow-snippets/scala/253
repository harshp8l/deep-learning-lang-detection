df.rdd
  .map( c=> (c.getAs[String](""MeteVarID"")  , c.getAs[String](""Conc"") ) )
  .reduceByKey( _ +"", ""+ _)
  .toDF(""MeteVarID"", ""Conc"")
  .show(false)

