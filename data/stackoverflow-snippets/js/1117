<div id=""app"">
  <button @click=""filter"">Filter</button>
</div>

new Vue({
  el: ""#app"",
  data: {
    specialityTest: [
      {person:'nurse', text:'text1'}, 
      {person:'nurse', text:'text1'}, 
      {person:'physician', text:'text1'}, 
      {person:'physician', text:'text1'}
    ]
  },
  methods: {
    filter() {
        let filtered = this.specialityTest.filter((el) => {
        return (el.person ==""physician"")
      })
      console.log(filtered)
    }
  }
})
