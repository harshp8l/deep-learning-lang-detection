  <tbody>
    {{#if rows}}
      {{#each rows as |row|}}
        <tr>
          <td><a href=""/keys/{{row.id}}/edit"" class=""""><i class=""fa fa-edit""></i></a></td>
          <td>{{row.status}}</td>
          <td>{{row.brand}}</td>
          <td>{{row.type}}</td>
          <td>{{row.label}}</td>
          {{#each ../langArray as |lang|}}
            <td>{{lookup (lookup row.translations lang) 'text'}} </td>
          {{/each}}
        </tr>
      {{/each}}
    {{else}}  
      <tr><td colspan=""100"" class=""text-center"">There are no keys to show.</td></tr>
    {{/if}}
  </tbody>
