<div id=""app"">
  <div v-for=""row in splitedList"">
    <p>{{ row.name }}</p>
    <div v-for=""code in row.codes"">
      <p>{{ code }}</p>
      </div>
  </div>
</div>

new Vue({
  el: ""#app"",
  data: {
    list : [
      {
        'name' : 'aaa',
        'codes' : '111+222+333'
      },
      {
        'name' : 'bbb',
        'codes' : '432+456+678'
      }
    ]
  },
  computed: {
    splitedList(){
        let newArr = [...this.list]
      newArr.map(el => {
        return el.codes = el.codes.split('+')
      })
      return newArr
    }
  }

})
