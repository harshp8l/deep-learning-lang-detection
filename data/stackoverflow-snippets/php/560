<table >
    <thead>
        <tr>
            <th >Word</th>
            <th >Frequency</th>   
        </tr>
    </thead>
        <?php
        $response = $db->query(""SELECT * FROM frequencies WHERE freq 
        BETWEEN 900 AND 910 ORDER BY freq"");

        ?>
    <tbody>
        <?php  
 while ( $row = $response->fetch()) { 
         ?>
           <tr>

            <td><?php echo $row['word']; ?></td>
            <td><?php echo $row['freq']; ?></td>

         </tr>
          <?php } 

            $response->closeCursor();

           ?>
    </tbody>
</table>
