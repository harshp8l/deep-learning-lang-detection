<script>
    $(document).ready(function() {
        $(""#submit"").click(function() {
            var a = ""h"";
            $.ajax({
                url: ""test.php"",
                method: ""POST"",
                data: {
                    data1: a
                },
                success: function(response) {
                   var data = jQuery.parseJSON(response).
                   $('#name').val(data.name);
                }
            });
        });
    });

</script>
