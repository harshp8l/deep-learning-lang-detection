switch($count) {
    case 3:
        echo '</tbody></table>';
        break;
    case 4:
        echo '<tr><td>row 4</td></tr></tbody></table>';
        break;
    case 5:
        echo '<tr><td>row 4</td></tr><tr><td>row 5</td></tr></tbody></table>';
        break;
    case $count > 5:
        echo '<tr><td>row 14</td></tr><tr><td>row 15</td></tr><tr><td>row 16</td></tr></tbody></table>';
        break;
}
