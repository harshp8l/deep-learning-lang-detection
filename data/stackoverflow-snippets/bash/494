first=yes
cat postfix > ""file.html"" #sic
echo '<table>' >> ""file.html""
cal -h | while read line ; do
    if [ $first = yes ] ; then
        first=no
        echo ""<tr><th colspan=\""7\"">$line</th></tr>"" >> ""file.html""
    else
        tdopen=${line//\(/<td>}
        tdclose=${tdopen//\)/<\/td>}
        echo ""<tr>$tdclose</tr>"" >> ""file.html""
    fi
done
echo '</table>' >> ""file.html""
cat prefix >> ""file.html"" #sic
