#!/bin/bash

a=0; b=0; c=0; d=0; e=0; f=0; g=0; h=0; i=0; j=0

while read -r -n1 char; do
    case ""$char"" in
        ""["" ) (( a++ )) ;;
        ""]"" ) (( b++ )) ;;
        ""("" ) (( c++ )) ;;
        "")"" ) (( d++ )) ;;
        ""{"" ) (( e++ )) ;;
        ""}"" ) (( f++ )) ;;
        ""'"" ) (( g++ )) ;;
        '""' ) (( h++ )) ;;
        '`' ) (( i++ )) ;;
        '/' ) (( j++ )) ;;
    esac
done <<< ""[]{}()/""$'\x60'$'\x22'$'\x27'     # $'\x60' == backtick

echo '[]'=""$a,$b""
echo '()'=""$c,$d""
echo '{}'=""$e,$f""
echo ""'"" =""$g""
echo '""' =""$h""
echo '`'=""$i""
echo '/'=""$j""
