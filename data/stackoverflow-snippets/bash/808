#!/bin/bash

tmpunsorted=""/tmp/compare_unsorted""
tmpsorted=""/tmp/compare_sorted""

>$tmpunsorted

while read linei
do
    while read linej
    do
        if [ $linei != $linej ]
        then
            echo $linei $linej | tr "" "" ""\n"" | sort | tr ""\n"" "" "" >>$tmpunsorted
            echo >>$tmpunsorted
        fi
    done <filelines
done <filelines

sort $tmpunsorted | uniq > $tmpsorted

while read linecompare
do
    echo ""./program $linecompare""
done <$tmpsorted

# Cleanup
rm -f $tmpunsorted
rm -f $tmpsorted
