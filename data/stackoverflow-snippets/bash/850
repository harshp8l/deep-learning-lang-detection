sh-4.4$ echo $string                                                                                                                                                                
foo ""bar_no_space"" ""baz with space""                                                                                                                                                 
sh-4.4$ echo $string|awk 'BEGIN{FS=""\""""}{for(i=1;i<NF;i++)print $i}'|sed '/^ $/d'                                                                                                   
foo                                                                                                                                                                                 
bar_no_space                                                                                                                                                                        
baz with space                                                                                                                                                                      
