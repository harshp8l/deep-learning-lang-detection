run=( CEADAPST CENIPT CEDAMT CEODST +ASM CEREMAFT );

arrayIn() {
    local value=$1;
    local -n array=$2;

    local found=0;

    for val in ""${array[@]}"";do
        [[ ""$val"" != ""$value"" ]] && {
            found=0;
        } || {
            found=1;
            break;
        }
    done

    (( found == 0 )) && return 1;
    return 0;
}

for dbname in CEREMAF CEREMAFT CEREMAFg +ASM;do
    arrayIn $dbname run
    (( $? == 0 )) && { echo ""yes""; } || echo ""nope""
done
