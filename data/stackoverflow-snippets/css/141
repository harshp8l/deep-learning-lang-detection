<div>       
  <p class=""test"" style=""background-color:#aaddff;"">Hello</p>
  <p class=""test"" style=""background-color:blue;"">Hello</p>
  <p class=""test"" style=""background-color:#aaddff;"">Hello</p>
</div>
<script type=""text/javascript"">
  $(function () {
      $('p').filter(function () {
       return $(this).css(""background-color"") === ""rgb(170, 221, 255)"";
      })
      .css(""border"", ""2px solid black"");
  });

</script>
