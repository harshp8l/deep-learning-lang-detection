// $(document) because the table is generated by script
$(document).on("click", "#check-count", function () {
    var count = 0;
    // Select elements - first <td> of each <tr> in the selected table - select checkbox
    // CARE!! the selector uses the table class .k-focusable - if there is another element
    // on the page (say, another table) then that will be selected as well.
    // To be more specific, it would be better to give this table an <id> if possible
    $.each($("table.k-focusable tbody tr").find("td:first input:checkbox"), function () {
        if ($(this).is(':checked')) {
            count += 1;
        }
    });

    alert("Number checked: " + count);
});
