template<typename T>
ostream& operator<<(ostream& out, const shared_ptr<T>& s_ptr)
{
    if (s_ptr != nullptr)
        out << (*s_ptr);
    return out;
}

template <typename T>
void print_vector(std::string text, std::vector<T> &vect){
    std::cout << "">>>>>>>>>> "" << text << "" <<<<<<<<<<"" << std::endl;
    for(T &t: vect){
        std::cout << t << "", "";
    }
    std::cout << std::endl << ""--------------------"" << std::endl;
}
