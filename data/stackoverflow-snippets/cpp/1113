#include <iostream>
#include <vector>
#include <cstdlib>
#include <ctime>

using namespace std;

int main()
{
    int num;
    int y = 0;

    cout << ""Enter the amount of numbers you would like generated: "" << endl;
    cin >> num;
    cout << endl;

    vector<int> x(num);    

    srand(time(0));

    for (int j = 0; j < num; ++j)
    {
        x[j] = (rand() % 10) + 1;
        if (x[j] == 1)
            ++y;
    }

    cout << ""1: "";
    for (int a = 0; a < y; ++a)
        cout << ""*"";
    cout << endl;

    return 0;
}
