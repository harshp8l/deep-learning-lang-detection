#include <iostream>
#include <vector>
#include <string>
#include <sstream>

using namespace std;

int main()
{
    cout << ""\nEnter n and n values: \n"";
    int n;
    cin >> n;
    vector<int> nums;
    int en = n;
    while (en > 0)
    {
        int x;
        cin >> x;
        nums.push_back(x);
        --en;
    }

    ostringstream new_val;
    if (n > 0)
    {
        new_val << nums[0];
        for (int i = 1; i < n; ++i)
            new_val << '0' << nums[i];
    }

    cout << new_val.str() << endl;
}
