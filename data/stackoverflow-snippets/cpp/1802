#include <iostream>
using namespace std;

int main()
{
const int SIZE = 10;
int number[SIZE];
int count;
int highest;

for (count = 0; count < SIZE; count++)
{
    cout << "Enter 10 numbers" << endl;
    cin >> number[count];
    cout << "you entered: " << number[count] << endl;
}

highest = number[0];
for (count = 1; count < SIZE; count++)
{
    if (number[count] > highest)
        highest = number[count];
}

cout << highest << endl;


return 0;
}
