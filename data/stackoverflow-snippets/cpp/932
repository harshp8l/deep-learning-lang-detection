#include <iostream>
#include <vector>
#include <random>

int main() {
    std::vector<std::normal_distribution<double>> D{ 
              std::normal_distribution<double>{0.0, 1.0 },                                                     
              std::normal_distribution<double>{0.0, 2.0 } };

    std::random_device rd;
    std::mt19937 gen(rd());

    std::cout << D[0](gen) << ""\n"";
    std::cout << D[1](gen) << ""\n"";

    return 0;
}
