// should be okay

unsigned int outerSwitch = someValue(); 
unsigned int innerSwitch = someOtherValue();
switch ( outerSwitch ) {
    case 1: {
        switch ( innerSwitch ) {
            case 1 : {
                // 1 or 2 lines okay;  
            } 
            case 2 : {
                // 1 or 2 lines okay;
            }
            case 3 : {
                // 1 or 2 lines okay;
            }
            default : {

            }
        } // inner switch when outer switch case = 1
    }
    case 2: {
          // Same as case 1
    }
    case 3: {
          // Same as case 1
    }
    default: {

    }
} // outer switch
