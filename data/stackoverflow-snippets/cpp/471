 #include <iostream>
    #include <cmath>
    #include <string>

    using namespace std;
    int digits(int zip);

    int main() {
        int zip = 0;
        cout << ""Enter zipcode: "";
        cin >> zip;
        int data = digits(zip);
        cout<<""test: ""<<data<<endl;
        return 0;
    }

    int digits(int zip){
        int d5 = int();
        d5 = zip % 10;
        // cout << ""test: "" << d5 << endl;
        return d5;
        }
