     #include <bits/stdc++.h>
    using namespace std;

    int main(){
        unsigned int a;
        unsigned char c;
        ifstream file;
        file.open(""ou.bin"", ios::binary);
        if(!file.is_open()){
            cout<<""error"";
            return 0;
        }
        for(int i=0; i<8; i++){
            file>>c;
            a = c;
            a = a<<8;
            file>>c;
            a = a+ c;
            cout<<a<<endl;
        }
        file.close();
        return 0;
   }
