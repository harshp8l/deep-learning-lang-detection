int main()
{
cout << ""Enter a number: "";
int number;
cin >> number;
int count = 1;
int a = 1;                  //The first number of the Fibonacci's serie is 1
int b = 1; 
int i = 1;                 //The second number of the Fibonacci's serie is 2
while (i <= number)
{
    if (i < 3)
        cout << ""1 "";
    else
    {
        count = a + b;     //Every number is the sum of the previous two
        a = b;
        b = count;
        cout << count << "" "";
    }
    i++;
}

return 0;
}
