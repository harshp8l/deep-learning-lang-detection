#include <iostream>
#include <cmath>
using namespace std;

void power_digit();

int main()
{
    power_digit();   
}
void power_digit()
{
    int x , y ,temp ;
    string result;
    cout << ""\nEnter number one :"";
    cin >> x ;
    cout << ""\nEnter number two :"";
    cin >> y ;
    cout << ""The power of x and y is : ""<< pow(x,y);
      for (int i = 1 ; i < y ; i++)
    {
        temp =  i ;
    }
    cout << ""\nThe digit of result is :"" << temp;
}
