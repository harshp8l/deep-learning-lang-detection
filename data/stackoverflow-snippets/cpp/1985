#include <iostream>
#include <vector>
#include <string>

#include ""Utility.h""

int main() {
    std::vector<std::tring> result;
    std::string str( ""This|is;a|test."" );
    std::cout << str << std::endl;

    result = Utility::splitString( str, "";"" );

    str.clear();
    for ( auto& s : result )
        str += s + "" "";
    std::cout << str << std::endl;

    result.clear();
    result = Utility::splitString( str, ""|"" );

    str.clear();
    for ( auto& s : result )
        str += s + "" "";
    std::cout << str << std::endl;

    system( ""PAUSE"" );
    return 0;
}
