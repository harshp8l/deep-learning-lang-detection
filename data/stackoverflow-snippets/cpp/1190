#include <math.h> 
#include <iostream>
using namespace std;

// driver program to test the above function
int main()
{


    uint32_t a, b;
    a = 625;
    b = 5;

    cout << ""The logarithm value(base-"" << b <<"") of "" << a
        << "" is "" << log(a,b) << endl;


    a = 1000;
    b = 10;

    cout << ""The logarithm value(base-"" << b << "") of "" << a
        << "" is "" << log(a, b) << endl;

    a = 243;
    b = 3;

    cout << ""The logarithm value(base-"" << b << "") of "" << a
        << "" is "" << log(a, b) << endl;


    return 0;
}
