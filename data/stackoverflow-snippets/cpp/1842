    #include <iostream>
    using namespace std;
    int main()
    {
        string alphabets=""abcdefghijklmnopqrstuvwxyz"" , 
        cipher=""ybjmxfwivclutakhdpsnzqerog"" , word , newword="""" ;
        cin>>word;
        newword=word;
        for(int i=0;i<26;i++)
        {
            for(int j=0;j<26;j++)
            {
                if(word[i]==alphabets[j])
                {
                    newword[i]=cipher[j];
                    break;
                }
            }
        }
        cout<<newword;
        return 0;
    }
