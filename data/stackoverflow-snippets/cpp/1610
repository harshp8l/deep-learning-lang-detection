    QString m_name;
    bool m_enabled;
    QList<QVariantMap> m_items;
    ...
};

void ComplexCurrency::read(const QJsonObject &json)
{
    m_id = json[""id""].toInt();
    m_name = json[""name""].toString();
    m_enabled = json[""enabled""].toBool();
    // parse items array here
}
