#include <iostream>

int xnor( unsigned int a, unsigned int b )
{
    const unsigned int Base = 10;
    unsigned int n = 1;
    unsigned int res = 0;

    while ( a and b )
    {
        res = res + n * ( a % Base and b % Base );
        n *= Base;
        a /= Base;
        b /= Base;
    }

    return res;
}

int main() 
{
    unsigned int a = 1101011;
    unsigned int b = 1010101;

    int res = xnor( a, b );

    std::cout << a << std::endl;
    std::cout << ""XNOR"" << std::endl;
    std::cout << b << std::endl;
    std::cout << ""========"" << std::endl;
    std::cout << res << std::endl;

    return 0;
}
