        QString line;

        QTextStream in(&inFile);
        QTextStream out(&outFile);
        out.setCodec(""UTF-8"");

        bool first = false;
        while (!in.atEnd())
        {
            if(first)
            {
                out << endl;
            }
            else
            {
                first = true;
            }
            line = in.readLine();
            out << line;
        }

        if(line.isEmpty())
        {
            out << endl;
        }
