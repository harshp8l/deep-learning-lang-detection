#include <algorithm>
#include <iterator>
#include <iostream>
#include <climits>

int main()
{
    int maxv = std::numeric_limits<int>::min();
    int high_index;
    int curIndex = 1;
    std::for_each(std::istream_iterator<int>{std::cin}, 
                  std::istream_iterator<int>{}, [&](int n) 
                  { 
                    if (maxv < n)
                    {
                        high_index = curIndex;
                        maxv = n;
                    }
                    ++curIndex; 
                });
    std::cout << ""Highest index: "" << high_index;
}
