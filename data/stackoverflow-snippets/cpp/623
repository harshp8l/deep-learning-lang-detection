#include <iostream>
//#include <iomanip> why?
using namespace std;
int main()
{
    int row_nr;
    cin >> row_nr;
    if(row_nr > 1 && row_nr <= 30)
        for(int i = 1; i <= row_nr; i++) //declare iterator variable in for loop statement
        {
            for(int j = 0; j < row_nr; j++)
            {
                cout << i + j * (row_nr);
                if(j < row_nr - 1) //you forgot this line
                {
                    cout << "" "";
                }
            }
            cout << '\n'; //endl flushes the buffer, unnecessary here
        }
    return 0;
}
