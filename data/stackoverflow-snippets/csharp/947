 <script type="text/javascript">

        function RemoveActivity(index) {

                    var urlBase = '/Dashboard/RemoveActivity/';
                    $.ajax({
                        url: urlBase,
                        data: { id: index },
                        success: function (result) {                             
                        },
                        error: function (ex) {
                        }
                    });
                }                

</script>
