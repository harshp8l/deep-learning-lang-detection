let inline wrapForDecode (buffer:string) 
                         (offset:int)
                         (blockLen:uint16)
                         (version:uint16)
                         : ^a =
  let msg = new ^a()
  (^a : (member WrapForDecode : string * int * int * int -> unit) 
      msg, 
      buffer, 
      offset, 
      (int blockLen), 
      (int version))
  msg

type foo () = 
    member this.WrapForDecode (a : string, b: int, c: int, d:int) = 
        ()

let x : foo = wrapForDecode ""asd"" 1 2us 3us
