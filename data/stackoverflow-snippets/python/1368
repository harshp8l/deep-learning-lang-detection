votes = [6]*28
m = 10
print(has_valid_subset(votes, m))
# True

votes1 = [5]*28+[6]*2
m1 = 10
print(has_valid_subset(votes1, m1))
# True

votes2 = [5]*29+[10]*1
m2 = 10
print(has_valid_subset(votes2, m2))
# False

