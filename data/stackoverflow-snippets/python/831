        for line in f:
            errorcode = 1
            while errorcode != 0:
                # Go back to here
                longUrl = line
                outputS = shurl(longUrl)    

                shorturl = outputS[0]
                errorcode = outputS[1]  

                if errorcode == 0:  
                    print(shorturl)
                    outfile.write(shorturl + ""\n"")
                else:
                    print(""ERROR, trying again."")
                    # Go back
