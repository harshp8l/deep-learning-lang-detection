user> (translate {:id 1
                  :content ""abc""
                  :severity ""101""
                  :translations [{:locale ""fr_FR""
                                  :content ""abc""}
                                 {:locale ""ru_RU""
                                  :content ""абв""}]}
                 ""ru_RU"")
;;=> {:id 1,
;;    :content ""абв"", 
;;    :severity ""101"", 
;;    :translations [{:locale ""fr_FR"", :content ""abc""} {:locale ""ru_RU"", :content ""абв""}]}
