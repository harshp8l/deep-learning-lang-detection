#Route{:info {:coercion :schema},
       :childs [#Route{:childs [#Route{:path ""/"",
                                       :info {:static-context? true},
                                       :childs [#Route{:path ""/myroute"",
                                                       :method :get,
                                                       :info {:name :my-get, :public {:x-name :my-get}}}
                                                #Route{:path ""/myroute"",
                                                       :method :post,
                                                       :info {:name :my-post, :public {:x-name :my-post}}}]}]}]}
