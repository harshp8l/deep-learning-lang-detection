public class CD {
  private int id;
  private int price;
  private String title;
  private String type;

  // constructor
  public CD (int id, int price, String title, String type){
      this.id = id;
      this.price = price;
      this.title = title;
      this.type = type;
  }

  public int getId() {
     return id;
  }

  public void setId(int id) {
     this.id = id;
  }

  public int getPrice() {
     return price;
  }

  public void setPrice(int price) {
     this.price = price;
  }

  public String getType() {
     return type;
  }

  public void setType(int type) {
     this.type = type;
  }

  public String getTitle() {
     return title;
  }

  public void setTitle(int title) {
     this.title = title;
  }

}
