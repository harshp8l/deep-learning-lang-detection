class ButtonListener implements ActionListener {
    private int guess;
    public ButtonListener(int guess) {
        this.guess = guess;
    }

    public void actionPerformed(ActionEvent e) {
        if (guess == winningNum) {
            showMessageDialog(""Equal!"");
        }
        if (guess < winningNum) {
            showMessageDialog(guess + "" < "" + winningNum);
        }
        if (guess > winningNum) {
            showMessageDialog(guess + "" > "" + winningNum);
        }
    }
}
