package main

import (
    ""fmt""
    ""net/http""
)

func main() {
    hello := ""hello yu bn""
    http.HandleFunc(""/"", func(w http.ResponseWriter, r *http.Request) {
        fmt.Fprintf(w, `<html>
            <head>
            </head>
            <body>
            <h1>Go Timer (ticks every second!)</h1>
            <div id=""output""></div>
            <script type=""text/javascript"">
            console.log(""`+hello+`"");
            </script>
            </body>
            </html>`)

    })
    http.ListenAndServe("":9000"", nil)
}
