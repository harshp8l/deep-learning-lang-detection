var (
    awesomeEnabled bool
)

func AwesomePlugin() *myplugin.MyPlugin {
    return &myplugin.MyPlugin{
        Name: ""AwesomePlugin"",
        Enable: func() error {
            println(""Enabling AwesomePlugin"")
            awesomeEnabled = true
            return nil // or do something more complex that could error
        },
        Disable: func() error {
            println(""Disabling AwesomePlugin"")
            awesomeEnabled = false
            return nil // or do something more complex that could error
        },
    }
}
