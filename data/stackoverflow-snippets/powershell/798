function Get-Result{
    param(
        [Parameter(
            ValueFromPipeline)]
        [string]$lookup
    )

    process{
        switch($lookup){
            ""first""{
                [pscustomobject]@{LookupValue=$lookup;Result=""one""},
                [pscustomobject]@{LookupValue=$lookup;Result=""two""}
            }
            default{
                [pscustomobject]@{LookupValue=$lookup;Result=""three""}
            }

        }
    }
}

$a = ""first"", ""second""
$a | Get-Result | Format-Table -GroupBy LookupValue
